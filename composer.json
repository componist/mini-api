{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "componist/mini-api",
    "description": "Config-driven read-only API for Laravel. Expose your data via simple GET requestsâ€”define routes, tables or Eloquent models, columns, and relations in one config file. Optional API key auth.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "api",
        "rest",
        "get",
        "read-only",
        "json",
        "config",
        "eloquent",
        "api-key",
        "auth"
    ],
    "homepage": "https://github.com/componist/mini-api",
    "support": {
        "issues": "https://github.com/componist/mini-api/issues",
        "source": "https://github.com/componist/mini-api"
    },
    "authors": [
        {
            "name": "Componist",
            "homepage": "https://github.com/componist"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Componist\\MiniApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Componist\\MiniApi\\MiniApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
